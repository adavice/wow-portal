}
.privacy .wrap {
	max-width: 760px;
    background: #fff;
    border: 1px solid #ccc;
    padding: 40px 30px;
    box-sizing: border-box;
    border-radius: 2px;
}
.privacy h2 {
	color: #0066ff !important;
    font-size: 14px;
    font-weight: 700;
